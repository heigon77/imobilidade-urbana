{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "none",
    "noImplicitAny": true,
    "outFile": "./build/build.js",
    "preserveConstEnums": true,
    "removeComments": true,
    "rootDir": "./sketch/",
    // "sourceMap": true,
    "target": "ESNext",
    "moduleResolution": "node",
    "lib": ["esnext", "DOM"],
    "composite": true,
    "noEmit": false
  },
  "include": ["./**/*"],
  "exclude": ["./node_modules/**/*", "./build/**/*"]
}
